169. Majority Element
class Solution {
public:
//This approach has a time complexity of  ğ‘‚(n) and space complexity of O(n) sc because of map
 int majorityElement(vector<int>& nums) {
       unordered_map<int,int> counts;
       int n=nums.size();
       for(int num:nums)
       {
        counts[num]++;
        if(counts[num]>n/2)
         {
            return num;
         }
       }
       return -1;
    }
};
//for space comp O(1) use "Boyerâ€“Moore majority vote algorithm" (candidate)
